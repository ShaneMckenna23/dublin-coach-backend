{
  "name": "dublin-coach-backend",
  "version": "1.0.0",
  "description": "Using latest JavaScript features on the server",
  "repository": "https://github.com/shanemckenna23/dublin-coach-backend",
  "author": "Shane Mckenna <shanemckenna23@hotmail.co.uk>",
  "license": "MIT",
  "main": "build/index.js",
  "engines": {
    "node": ">=7",
    "npm": ">=4"
  },
  "scripts": {
    "prestart": "npm run -s build",
    "start": "node build/index.js",
    "dev":
      "nodemon src/index.js --exec \"node -r dotenv/config -r babel-register\"",
    "clean": "rimraf build",
    "build": "npm run clean && mkdir build && babel src -s -D -d build",
    "build-prod": "npm run clean && mkdir build && babel src -D -d build",
    "test-build": "jest",
    "start-prod":
      "npm run build-prod && cross-env NODE_ENV=production node build/index.js",
    "test": "jest",
    "lint": "esw -w src test",
    "gcloud-auth": "node config/gcloud.js"
  },
  "dependencies": {
    "apollo-engine": "^0.5.0",
    "apollo-server-express": "^1.1.2",
    "babel-cli": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "bcrypt": "^1.0.3",
    "bindings": "^1.3.0",
    "bluebird": "^3.5.0",
    "body-parser": "^1.17.1",
    "compression": "^1.7.1",
    "connect-redis": "^3.3.0",
    "cookie-parser": "^1.4.3",
    "cors": "^2.8.4",
    "cross-env": "^5.1.3",
    "dataloader": "^1.3.0",
    "express": "^4.15.2",
    "express-flash": "^0.0.2",
    "express-graphql": "^0.6.11",
    "express-session": "^1.15.5",
    "faker": "^4.1.0",
    "graphql": "^0.11.3",
    "graphql-relay": "^0.5.2",
    "graphql-tools": "^1.2.3",
    "handlebars": "^4.0.10",
    "handlebars-layouts": "^3.1.4",
    "i18next": "^9.0.0",
    "i18next-express-middleware": "^1.0.6",
    "i18next-node-fs-backend": "^1.0.0",
    "jest": "^21.1.0",
    "jsonwebtoken": "^8.1.0",
    "juice": "^4.1.1",
    "knex": "^0.13.0",
    "morgan": "^1.7.0",
    "multer": "^1.3.0",
    "mysql2": "^1.4.2",
    "node-addon-api": "^0.6.3",
    "node-fetch": "^1.7.3",
    "nodemailer": "^4.1.0",
    "passport": "^0.4.0",
    "passport-facebook": "^2.1.1",
    "passport-google-oauth": "^1.0.0",
    "passport-twitter": "^1.0.4",
    "pg": "^7.3.0",
    "pretty-error": "^2.1.1",
    "pug": "^2.0.0-beta11",
    "redis": "^2.8.0",
    "rimraf": "^2.6.1",
    "sequelize": "^4.12.0",
    "source-map-support": "^0.4.18",
    "validator": "^8.2.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^7.2.3",
    "babel-jest": "^21.0.2",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-export-extensions": "^6.22.0",
    "babel-plugin-transform-flow-strip-types": "^6.22.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "babel-register": "^6.24.1",
    "chokidar": "^1.7.0",
    "del": "3.0.0",
    "dotenv": "^4.0.0",
    "eslint": "^4.6.1",
    "eslint-config-airbnb-base": "^12.0.0",
    "eslint-config-prettier": "^2.5.0",
    "eslint-plugin-flowtype": "^2.35.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jest": "^21.0.2",
    "eslint-plugin-prettier": "^2.2.0",
    "eslint-watch": "^3.1.0",
    "flow-bin": "^0.54.1",
    "husky": "^0.14.3",
    "lint-staged": "^4.2.1",
    "nodemon": "^1.11.0",
    "prettier": "^1.7.0",
    "supertest": "^3.0.0"
  },
  "babel": {
    "presets": [
      [
        "env",
        {
          "targets": {
            "node": "current"
          }
        }
      ]
    ],
    "plugins": ["transform-object-rest-spread"]
  },
  "eslintConfig": {
    "parser": "babel-eslint",
    "plugins": ["import", "jest"],
    "parserOptions": {
      "ecmaVersion": 2017,
      "sourceType": "module"
    },
    "env": {
      "node": true,
      "jest": true
    },
    "extends": ["eslint:recommended"],
    "rules": {
      "jest/no-focused-tests": 2,
      "jest/no-identical-title": 2
    }
  },
  "jest": {
    "testEnvironment": "node"
  }
}
